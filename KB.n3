@prefix MyBase: <file:///U:/7%20%D1%81%D0%B5%D0%BC%D0%B5%D1%81%D1%82%D1%80/pythonProject/MyBase/#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs1: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix formulas: <file:///U:/7%20%D1%81%D0%B5%D0%BC%D0%B5%D1%81%D1%82%D1%80/pythonProject/MyBase/formulas/#> .


formulas:formula_name a rdf:Property .
formulas:parametrs a rdf:Property .

formulas:formula a rdfs1:Class ;
    rdfs1:domain formulas:parametrs ;
    rdfs1:domain formulas:formula_name .

formulas:plus a formulas:formula ;
    formulas:formula_name "plus" ;
    formulas:numparam 3 ;
    formulas:parametrs  [
    a rdf:Seq ;
    rdf:_3 "z";
    rdf:_1 "x";
    rdf:_2 "y"
     ].